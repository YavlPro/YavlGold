{
  "cleanUrls": true,
  "trailingSlash": true,
  "redirects": [
    { "source": "/herramientas", "destination": "/tecnologia", "permanent": true },
    { "source": "/herramientas/:path*", "destination": "/tecnologia/:path*", "permanent": true }
  ],
  "rewrites": [
    { "source": "/academia", "destination": "/academia/index.html" },
    { "source": "/academia/", "destination": "/academia/index.html" },
    { "source": "/academia/:path*", "destination": "/academia/:path*" },
    { "source": "/agro", "destination": "/agro/index.html" },
    { "source": "/agro/", "destination": "/agro/index.html" },
    { "source": "/agro/:path*", "destination": "/agro/:path*" },
    { "source": "/crypto", "destination": "/crypto/index.html" },
    { "source": "/crypto/", "destination": "/crypto/index.html" },
    { "source": "/crypto/:path*", "destination": "/crypto/:path*" },
    { "source": "/dashboard", "destination": "/dashboard/index.html" },
    { "source": "/dashboard/", "destination": "/dashboard/index.html" },
    { "source": "/dashboard/perfil", "destination": "/dashboard/perfil.html" },
    { "source": "/dashboard/perfil/", "destination": "/dashboard/perfil.html" },
    { "source": "/dashboard/perfil.html", "destination": "/dashboard/perfil.html" },
    { "source": "/dashboard/configuracion", "destination": "/dashboard/configuracion.html" },
    { "source": "/dashboard/configuracion/", "destination": "/dashboard/configuracion.html" },
    { "source": "/dashboard/configuracion.html", "destination": "/dashboard/configuracion.html" },
    { "source": "/dashboard/:path*", "destination": "/dashboard/:path*" },
    { "source": "/music", "destination": "/dashboard/music.html" },
    { "source": "/music/", "destination": "/dashboard/music.html" },
    { "source": "/tecnologia", "destination": "/tecnologia/index.html" },
    { "source": "/tecnologia/", "destination": "/tecnologia/index.html" },
    { "source": "/tecnologia/:path*", "destination": "/tecnologia/:path*" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-YG-Config",
          "value": "apps-gold-vercel-active"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "geolocation=(), microphone=(), camera=()"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/agro",
      "headers": [
        {
          "key": "Permissions-Policy",
          "value": "geolocation=(self), microphone=(), camera=()"
        }
      ]
    },
    {
      "source": "/agro/",
      "headers": [
        {
          "key": "Permissions-Policy",
          "value": "geolocation=(self), microphone=(), camera=()"
        }
      ]
    },
    {
      "source": "/agro/:path*",
      "headers": [
        {
          "key": "Permissions-Policy",
          "value": "geolocation=(self), microphone=(), camera=()"
        }
      ]
    }
  ]
}
