{
  "buildCommand": "pnpm build:v9",
  "outputDirectory": "apps/gold/dist",
  "framework": null,
  "installCommand": "pnpm install",
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "yavlgold.gold" }],
      "destination": "https://yavlgold.com/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "www.yavlgold.gold" }],
      "destination": "https://yavlgold.com/:path*",
      "permanent": true
    },
    {
      "source": "/herramientas",
      "destination": "/tecnologia",
      "permanent": true
    },
    {
      "source": "/herramientas/:path*",
      "destination": "/tecnologia/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/academia", "destination": "/academia/index.html" },
    { "source": "/academia/:path*", "destination": "/academia/:path*" },
    { "source": "/agro", "destination": "/agro/index.html" },
    { "source": "/agro/:path*", "destination": "/agro/:path*" },
    { "source": "/tecnologia", "destination": "/tecnologia/index.html" },
    { "source": "/tecnologia/:path*", "destination": "/tecnologia/:path*" },
    { "source": "/herramientas", "destination": "/herramientas/index.html" },
    { "source": "/herramientas/:path*", "destination": "/herramientas/:path*" },
    { "source": "/suite", "destination": "/suite/index.html" },
    { "source": "/suite/:path*", "destination": "/suite/:path*" },
    { "source": "/social", "destination": "/social/index.html" },
    { "source": "/social/:path*", "destination": "/social/:path*" },
    { "source": "/dashboard", "destination": "/dashboard/index.html" },
    { "source": "/dashboard/configuracion", "destination": "/dashboard/configuracion.html" },
    { "source": "/dashboard/perfil", "destination": "/dashboard/perfil.html" },
    { "source": "/profile", "destination": "/profile/index.html" },
    { "source": "/cookies", "destination": "/cookies.html" },
    { "source": "/faq", "destination": "/faq.html" },
    { "source": "/roadmap", "destination": "/roadmap.html" }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/(.*).html",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
      ]
    }
  ]
}
