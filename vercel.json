{
  "buildCommand": "pnpm build:v9",
  "outputDirectory": "apps/gold/dist",
  "framework": null,
  "installCommand": "pnpm install",
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "yavlgold.gold" }],
      "destination": "https://yavlgold.com/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "www.yavlgold.gold" }],
      "destination": "https://yavlgold.com/:path*",
      "permanent": true
    },
    {
      "source": "/herramientas",
      "destination": "/tecnologia",
      "permanent": true
    },
    {
      "source": "/herramientas/:path*",
      "destination": "/tecnologia/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/music", "destination": "/dashboard/music.html" },
    { "source": "/music/", "destination": "/dashboard/music.html" }
  ],
  "headers": [
    {
      "source": "/",
      "headers": [
        { "key": "X-YG-Config", "value": "root-vercel-active" },
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "X-XSS-Protection", "value": "1; mode=block" },
        { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" },
        { "key": "Permissions-Policy", "value": "geolocation=(), microphone=(), camera=()" }
      ]
    },
    {
      "source": "/agro",
      "headers": [
        { "key": "X-YG-Config", "value": "root-vercel-active" },
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "X-XSS-Protection", "value": "1; mode=block" },
        { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" },
        { "key": "Permissions-Policy", "value": "geolocation=(self), microphone=(), camera=()" }
      ]
    },
    {
      "source": "/agro/",
      "headers": [
        { "key": "X-YG-Config", "value": "root-vercel-active" },
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "X-XSS-Protection", "value": "1; mode=block" },
        { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" },
        { "key": "Permissions-Policy", "value": "geolocation=(self), microphone=(), camera=()" }
      ]
    },
    {
      "source": "/agro/:path*",
      "headers": [
        { "key": "X-YG-Config", "value": "root-vercel-active" },
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "X-XSS-Protection", "value": "1; mode=block" },
        { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" },
        { "key": "Permissions-Policy", "value": "geolocation=(self), microphone=(), camera=()" }
      ]
    },
    {
      "source": "/:path((?!agro(?:/|$)).+)",
      "headers": [
        { "key": "X-YG-Config", "value": "root-vercel-active" },
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "X-XSS-Protection", "value": "1; mode=block" },
        { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" },
        { "key": "Permissions-Policy", "value": "geolocation=(), microphone=(), camera=()" }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/(.*).html",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
      ]
    }
  ]
}
