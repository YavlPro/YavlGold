{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "vite",
  "installCommand": "pnpm install",
  "buildCommand": "test ! -f index.html && pnpm -C apps/gold build",
  "outputDirectory": "apps/gold/dist",
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
