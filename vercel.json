{
  "buildCommand": "pnpm build:v9",
  "outputDirectory": "apps/gold/dist",
  "framework": null,
  "installCommand": "pnpm install",
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "yavlgold.gold" }],
      "destination": "https://yavlgold.com/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "www.yavlgold.gold" }],
      "destination": "https://yavlgold.com/:path*",
      "permanent": true
    },
    {
      "source": "/herramientas",
      "destination": "/tecnologia",
      "permanent": true
    },
    {
      "source": "/herramientas/:path*",
      "destination": "/tecnologia",
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/music", "destination": "/dashboard/music.html" },
    { "source": "/music/", "destination": "/dashboard/music.html" }
  ],
  "headers": [
    {
      "source": "/:path*",
      "headers": [
        { "key": "X-YG-Config", "value": "root-vercel-active" }
      ]
    }
  ]
}
